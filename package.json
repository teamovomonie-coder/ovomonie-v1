{
  "name": "ovomonie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node -r ./self-polyfill.js ./node_modules/next/dist/bin/next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:check": "npm run lint:fix && npm run build",
    "build:force": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix || true",
    "format": "prettier --write . || true",
    "ci:fix": "npm run lint:fix && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:load": "k6 run load-test.js",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__ --watchAll=false",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run lint:check && npm run typecheck && npm run test:unit && npm run test:e2e",
    "perf:analyze": "npm run build:analyze && npx @next/bundle-analyzer .next",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "monitor:health": "node -e \"require('./src/lib/monitoring').monitoring.getPerformanceSummary()\"",
    "fix:auth": "node scripts/fix-auth.js",
    "test:vfd": "node scripts/test-vfd-connectivity.js",
    "debug:vfd": "node scripts/debug-vfd-api.js",
    "debug:vfd-withdrawal": "node scripts/debug-vfd-withdrawal.js",
    "tx:view": "node scripts/view-transaction.js",
    "tx:update": "node scripts/update-transaction-status.js",
    "vercel:build": "npm run build:check"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.27.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.1.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/supabase-js": "^2.39.0",
    "@types/jest": "^30.0.0",
    "@types/nprogress": "^0.2.3",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.0.0",
    "framer-motion": "^10.18.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.475.0",
    "next": "15.1.6",
    "node-fetch": "^3.3.2",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.45.0",
    "recharts": "^2.10.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "wav": "^1.0.2",
    "zod": "^3.23.0"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "15.1.6",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3"
  }
}
