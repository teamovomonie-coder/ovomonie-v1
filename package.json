{
  "name": "ovomonie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix || true",
    "format": "prettier --write . || true",
    "ci:fix": "npm run lint:fix && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:load": "k6 run load-test.js",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__ --watchAll=false",
    "test:integration": "jest --testPathPattern=integration --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run lint && npm run typecheck && npm run test:unit && npm run test:e2e",
    "perf:analyze": "npm run build:analyze && npx @next/bundle-analyzer .next",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "monitor:health": "node -e \"require('./src/lib/monitoring').monitoring.getPerformanceSummary()\"",
    "fix:auth": "node scripts/fix-auth.js",
    "test:vfd": "node scripts/test-vfd-connectivity.js",
    "debug:vfd": "node scripts/debug-vfd-api.js",
    "debug:vfd-withdrawal": "node scripts/debug-vfd-withdrawal.js",
    "db:seed": "node scripts/seed-database.js",
    "migrate:import-supabase": "node scripts/migrate-import-supabase.js",
    "migrate:verify": "node scripts/migrate-verify.js",
    "audit:security": "npm audit && npm run test:security",
    "test:security": "jest --testPathPattern=security",
    "cache:clear": "node -e \"require('./src/lib/cache').cache.clear(); console.log('Cache cleared');\"",
    "health:check": "curl http://localhost:3000/api/health",
    "tx:view": "node scripts/view-transaction.js",
    "tx:update": "node scripts/update-transaction-status.js"
  },
  "dependencies": {
    "@dataconnect/admin-generated": "file:src/dataconnect-admin-generated",
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@supabase/supabase-js": "^2.87.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.2.1",
    "framer-motion": "^11.2.10",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "dompurify": "^3.0.4",
    "lucide-react": "^0.475.0",
    "mongodb": "^7.0.0",
    "next": "15.5.9",
    "nprogress": "^0.2.0",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "supabase": "^2.67.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "wav": "^1.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/wav": "^1.0.3",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.41.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.7",
    "genkit-cli": "^1.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "@next/bundle-analyzer": "^15.5.7",
    "cross-env": "^7.0.3",
    "@types/webauthn": "^0.0.1"
  },
  "overrides": {
    "next": "15.5.9"
  }
}
