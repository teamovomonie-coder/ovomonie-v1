{
  "name": "ovomonie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:check": "npm run lint:fix && npm run build",
    "build:force": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix || true",
    "lint:check": "next lint",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write . || true",
    "ci:fix": "npm run lint:fix && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:load": "k6 run load-test.js",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__ --watchAll=false",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run lint:check && npm run typecheck && npm run test:unit && npm run test:e2e",
    "perf:analyze": "npm run build:analyze && npx @next/bundle-analyzer .next",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "monitor:health": "node -e \"require('./src/lib/monitoring').monitoring.getPerformanceSummary()\"",
    "fix:auth": "node scripts/fix-auth.js",
    "test:vfd": "node scripts/test-vfd-connectivity.js",
    "debug:vfd": "node scripts/debug-vfd-api.js",
    "debug:vfd-withdrawal": "node scripts/debug-vfd-withdrawal.js",
    "db:seed": "node scripts/seed-database.js",
    "migrate:import-supabase": "node scripts/migrate-import-supabase.js",
    "migrate:verify": "node scripts/migrate-verify.js",
    "migrate:inventory": "node scripts/migrate-inventory.js",
    "audit:security": "npm audit && npm run test:security",
    "test:security": "jest --testPathPattern=security",
    "cache:clear": "node -e \"require('./src/lib/cache').cache.clear(); console.log('Cache cleared');\"",
    "health:check": "curl http://localhost:3000/api/health",
    "tx:view": "node scripts/view-transaction.js",
    "tx:update": "node scripts/update-transaction-status.js",
    "vercel:build": "npm run build:check"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/postcss": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^10.12.16",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.257.0",
    "next": "15.5.9",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.45.0",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.23",
    "eslint": "^8",
    "eslint-config-next": "15.1.6",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3"
  }
}